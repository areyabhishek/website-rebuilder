  <main class="main">
    <section class="hero">
      <div class="container">
        <h1 class="hero-title">Delete File</h1>
        <p class="hero-description">API reference for deleting files in Vapi</p>
      </div>
    </section>

    <section class="section">
      <div class="container">
        <div class="card">
          <h2>API Explorer</h2>
          
          <div class="api-section">
            <h3>Authentication</h3>
            <div class="auth-info">
              <div class="auth-type">
                <strong>Authorization</strong>
                <span class="auth-method">Bearer</span>
              </div>
              <p>Retrieve your API Key from <a href="https://dashboard.vapi.ai" class="link" target="_blank">Dashboard</a>.</p>
            </div>
          </div>

          <div class="api-section">
            <h3>Path parameters</h3>
            <div class="parameter">
              <div class="param-header">
                <code class="param-name">id</code>
                <span class="param-type">string</span>
                <span class="param-required">Required</span>
              </div>
              <p class="param-description">The unique identifier of the file to delete</p>
            </div>
          </div>

          <div class="api-section">
            <h3>Response</h3>
            <div class="response-info">
              <div class="response-field">
                <code class="field-name">id</code>
                <span class="field-type">string</span>
                <p class="field-description">The ID of the deleted file</p>
              </div>
            </div>
          </div>

          <div class="api-section">
            <h3>Example Request</h3>
            <pre class="code-block">DELETE /files/{id}
Authorization: Bearer YOUR_API_KEY
Content-Type: application/json</pre>
          </div>

          <div class="api-section">
            <h3>Example Response</h3>
            <pre class="code-block">{
  "id": "file_123456789",
  "deleted": true,
  "message": "File successfully deleted"
}</pre>
          </div>

          <div class="api-section">
            <h3>Error Responses</h3>
            <div class="error-examples">
              <div class="error-item">
                <h4>404 Not Found</h4>
                <pre class="code-block error">{
  "error": {
    "code": "file_not_found",
    "message": "The specified file could not be found"
  }
}</pre>
              </div>
              
              <div class="error-item">
                <h4>401 Unauthorized</h4>
                <pre class="code-block error">{
  "error": {
    "code": "unauthorized",
    "message": "Invalid API key provided"
  }
}</pre>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>

    <section class="section">
      <div class="container">
        <div class="card">
          <h2>Code Examples</h2>
          
          <div class="code-examples">
            <div class="example-section">
              <h3>JavaScript</h3>
              <pre class="code-block">const response = await fetch(`https://api.vapi.ai/files/${fileId}`, {
  method: 'DELETE',
  headers: {
    'Authorization': 'Bearer YOUR_API_KEY',
    'Content-Type': 'application/json'
  }
});

if (response.ok) {
  const result = await response.json();
  console.log('File deleted:', result.id);
} else {
  console.error('Failed to delete file:', response.statusText);
}</pre>
            </div>

            <div class="example-section">
              <h3>Python</h3>
              <pre class="code-block">import requests

file_id = "file_123456789"
api_key = "YOUR_API_KEY"

response = requests.delete(
    f"https://api.vapi.ai/files/{file_id}",
    headers={
        "Authorization": f"Bearer {api_key}",
        "Content-Type": "application/json"
    }
)

if response.status_code == 200:
    result = response.json()
    print(f"File deleted: {result['id']}")
else:
    print(f"Failed to delete file: {response.text}")</pre>
            </div>

            <div class="example-section">
              <h3>cURL</h3>
              <pre class="code-block">curl -X DELETE "https://api.vapi.ai/files/file_123456789" \
  -H "Authorization: Bearer YOUR_API_KEY" \
  -H "Content-Type: application/json"</pre>
            </div>
          </div>
        </div>
      </div>
    </section>
  </main>

